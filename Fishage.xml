<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Script file="FishageInit.lua"/>
    <Script file="FishageTracking.lua"/>
    <!-- Frame to handle all core events --> 
    <Frame name="FishageRoot" enableMouse="false" movable="true" parent="UIParent" hidden="true">
        <Scripts>
            <OnLoad>
                Fishage.OnLoad(self);
            </OnLoad>
            <OnEvent>
                Fishage.OnEvent(self, event, ...);
            </OnEvent>
        </Scripts>
    </Frame>
</Ui>
